container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && cd /var/app/staging && echo 'DATABASE_URL='$DATABASE_URL && python -c \"from config.settings import settings; print('DB URL:', settings.database_url[:50] + '...')\" && alembic upgrade head"
    leader_only: true
    ignoreErrors: true
